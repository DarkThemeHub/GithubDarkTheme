{
    "manifest_version": 2,
    "name": "GithubDarkTheme",
    "description": "Github Dark Theme",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/eventPage.js"],
        "persistent": false
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "notifications",
        "tabs",
        "alarms",
        "*://*.github.com/*",
        "*://*.github.com/",
        "*://github.com/",
        "*://github.com/*"
    ],
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [
                "*://*.github.com/*",
                "*://*.github.com/",
                "*://github.com/",
                "*://github.com/*"
            ],
            "js": ["public/injector.js"]
        }
    ]
}
